{
  "expo": {
    "name": "socialPulse-Client",
    "slug": "socialPulse-Client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/icon.png",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "src/assets/favicon.png"
    },
    "description": "Will need to clone the server side of the application: https://github.com/golden-state0fmind/SocialPulse-server",
    "githubUrl": "https://github.com/golden-state0fmind/SocialPulse-Client"
  }
}
